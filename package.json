    "axios": "^1.7.9",
    "hugeicons-react": "^0.3.0",
    "lodash": "^4.17.21",
    "next": "15.1.7",
    "next-themes": "^0.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.0.0",
